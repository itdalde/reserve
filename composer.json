{
    "name": "labs64/laravel-boilerplate",
    "description": "Laravel Boilerplate",
    "type": "project",
    "keywords": ["php", "framework", "laravel", "laravel7", "admin", "theme", "starter", "bootstrap", "boilerplate", "docker", "template", "social-login", "nodejs"],
    "homepage": "https://github.com/Labs64/laravel-boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Labs64 GmbH",
            "email": "info@labs64.com",
            "homepage": "https://www.labs64.com"
        }
    ],
    "require": {
        "php": ">=8.1.2",
        "albertcht/invisible-recaptcha": "^1.9",
        "arcanedev/log-viewer": "^8.0",
        "creativeorange/gravatar": "^1.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "fruitcake/laravel-cors": "^1.0",
        "fzaninotto/faker": "^1.5",
        "google/apiclient": "^2.12",
        "infobip-community/infobip-api-php-sdk": "^1.1",
        "kutia-software-company/larafirebase": "^1.3",
        "kyslik/column-sortable": "^6.1",
        "labs64/netlicensingclient-php": "^2.4",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.5",
        "laravel/passport": "*",
        "laravel/socialite": "^5.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1.0",
        "yajra/laravel-datatables": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.3",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-watcher": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Traits\\": "database/traits/"
        },
        "files": [
            "app/helpers.php",
            "app/Helpers/Common/Function.php",
            "app/Helpers/Common/GeneralHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
